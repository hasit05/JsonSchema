                    "fixedAmountEndDate": {
                      "type": "string",
					            "pattern": "^([0]?[1-9]|[1|2][0-9]|[3][0|1])[.\/-]([0]?[1-9]|[1][0-2])[.\/-]([0-9]{4}|[0-9]{2})$"
                    },
                    "faceValuePct": {
                      "type": "integer",
                      "format": "int64"
                    },
                    "fixedAmount": {
                      "type": "integer",
                      "format": "int64"
                    },
                    "faceValueProvSched": {
                      "type": "string"
                    },
                    "faceValueReturnSched": {
                      "type": "string"
                    },
                    "includeMarkToMarket": {
