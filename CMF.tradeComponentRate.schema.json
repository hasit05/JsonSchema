 {
              "$id": "/common-resources/schema/CMF.tradeComponentRate.schema.json",
      "$schema": "http://json-schema.org/draft-04/schema#",
                
                  "type": "object",
                  "properties": {
                    "startDate": {
                      "type": "string",
                      "pattern": "^([0]?[1-9]|[1|2][0-9]|[3][0|1])[.\/-]([0]?[1-9]|[1][0-2])[.\/-]([0-9]{4}|[0-9]{2})$"
                    },
                    "endDate": {
                      "type": "string",
					            "pattern": "^([0]?[1-9]|[1|2][0-9]|[3][0|1])[.\/-]([0]?[1-9]|[1][0-2])[.\/-]([0-9]{4}|[0-9]{2})$"
                    },
                    "period": {
                      "type": "integer"
                    },
                    "demand": {
                      "type": "number"
                    },
                    "strikePrice": {
                      "type": "number"
                    }
                  },
                  "required": [
                    "startDate",
                    "endDate",
                    "period",
                    "demand",
                    "strikePrice"
                  ]
                
     
            }